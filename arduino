const int ledPin = 10;
String sprava;
int IntSprava;

void setup() {
  pinMode(ledPin, OUTPUT);
  Serial.begin(9600);
}

void loop() {
  if (Serial.available() > 0) {
    sprava = Serial.readString();
    Serial.print("Sprava ako string = ");
    Serial.println(sprava); // Výpis reťazca

    IntSprava = sprava.toInt(); // Konverzia na číslo
    Serial.print("Sprava ako int = ");
    Serial.println(IntSprava); // Výpis čísla
  }
}
